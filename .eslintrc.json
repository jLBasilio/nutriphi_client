{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": ["error", { "functions": false }],
    "no-underscore-dangle": "off",
    "comma-dangle": ["error", "never"],
    "react/prop-types": 0,
    "no-nested-ternary": "off"
  }
}